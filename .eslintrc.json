{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "brace-style": "off",
    "new-cap": ["error", { "capIsNewExceptions": ["Polymer"] }],
    "no-var": "off",
    "require-jsdoc": "off"
  },
  "globals": {
    "Polymer": true,
    "jQuery": true,
    "moment": true,
    "buffer": true,
    "axios": true,
    "KbUtilBehavior": true,
    "FastClick": true,
    "io": true
  }
}
